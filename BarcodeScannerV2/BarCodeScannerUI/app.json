{
  "expo": {
    "name": "EthicalBarcodeScanner",
    "slug": "EthicalBarcodeScanner",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splashmoral.png",
      "resizeMode": "contain",
      "backgroundColor": "#0d1d1f"
    },


    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-barcode-scanner",
        {
          "cameraPermission": "Allow Ethical Barcode Scanner to access camera."
        }
      ]
    ]
  }
}
